{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "0.1.1-canary.0",
  "packages": [
    "packages/*",
    "templates/*"
  ],
  "registry": "https://registry.npmjs.org",
  "command": {
    "publish": {
      "ignoreChanges": [
        "**/__tests__/**",
        "**/*.test.js",
        "**/*.spec.js"
      ]
    },
    "version": {
      "push": false,
      "gitTagVersion": false
    },
    "run": {
      "npmClient": "npm",
      "private": false,
      "skipNxCache": false,
      "stream": true
    }
  }
}